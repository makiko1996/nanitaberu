<div class="body">
  <div class="tastes__all">
    <h3>オモさ変更</h3>
    <div class="tastes__form">
      <%= form_with model:@taste, url:admin_taste_path, local:true do |f| %>
        <table>
          <tr>
            <th>オモさ名称</th>
            <th><%= f.text_field :name %></th>
            <th><%= f.submit "変更する" %></th>
          </tr>
        </table>
      <% end %>
    </div>
  </div>
</div>
