<div class="card_group">
  <% posts.each do |post| %>
    <div class="card">
      <%= link_to post_path(post) do %>
        <%= attachment_image_tag post, :image, size:'250x200' %></br>
        <%= post.cooking_name %>
        <%= post.display_difficulty %>
      <% end %>
    </div>
  <% end %>
</div>
